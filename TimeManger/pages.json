{
	"pages": [
		{
			"path": "pages/splash/index",
			"style": {
				"navigationBarTitleText": "启动中",
				"navigationStyle": "custom",
				"enablePullDownRefresh": false
			}
		},
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "今日",
				"navigationStyle": "custom",
				"enablePullDownRefresh": false
			}
		},
		{
			"path": "pages/calendar/index",
			"style": {
				"navigationBarTitleText": "日历",
				"navigationStyle": "custom",
				"enablePullDownRefresh": false,
				"disableScroll": false
			}
		},
		{
			"path": "pages/pomodoro/index",
			"style": {
				"navigationBarTitleText": "番茄钟",
				"navigationStyle": "custom",
				"enablePullDownRefresh": false
			}
		},
		{
			"path": "pages/habit/index",
			"style": {
				"navigationBarTitleText": "习惯",
				"navigationStyle": "custom",
				"enablePullDownRefresh": false
			}
		}
	],
	"preloadRule": {
		"pages/splash/index": {
			"network": "all",
			"packages": ["__APP__"],
			"pages": [
				"pages/index/index",
				"pages/pomodoro/index",
				"pages/habit/index",
				"pages/calendar/index"
			]
		},
		"pages/index/index": {
			"network": "all",
			"packages": ["__APP__"],
			"pages": [
				"pages/pomodoro/index",
				"pages/habit/index",
				"pages/calendar/index"
			]
		},
		"pages/calendar/index": {
			"network": "all",
			"packages": ["__APP__"],
			"pages": [
				"pages/pomodoro/index",
				"pages/habit/index"
			]
		},
		"pages/pomodoro/index": {
			"network": "all",
			"packages": ["__APP__"],
			"pages": [
				"pages/calendar/index",
				"pages/habit/index"
			]
		},
		"pages/habit/index": {
			"network": "all",
			"packages": ["__APP__"],
			"pages": [
				"pages/calendar/index",
				"pages/pomodoro/index"
			]
		}
	},
	"globalStyle": {
		"navigationBarTextStyle": "white",
		"navigationBarTitleText": "TimeManager",
		"navigationBarBackgroundColor": "transparent",
		"backgroundColor": "#0a0e27"
	},
	"tabBar": {
		"color": "#8F9BB3",
		"selectedColor": "#FFFFFF",
		"backgroundColor": "rgba(255, 255, 255, 0.05)",
		"borderStyle": "white",
		"list": [
			{
				"pagePath": "pages/index/index",
				"text": "今日",
				"iconPath": "static/tabbar/home.png",
				"selectedIconPath": "static/tabbar/home-active.png"
			},
			{
				"pagePath": "pages/calendar/index",
				"text": "日历",
				"iconPath": "static/tabbar/home.png",
				"selectedIconPath": "static/tabbar/home-active.png"
			},
			{
				"pagePath": "pages/pomodoro/index",
				"text": "番茄钟",
				"iconPath": "static/tabbar/stats.png",
				"selectedIconPath": "static/tabbar/stats-active.png"
			},
			{
				"pagePath": "pages/habit/index",
				"text": "习惯",
				"iconPath": "static/tabbar/user.png",
				"selectedIconPath": "static/tabbar/user-active.png"
			}
		]
	},
	"uniIdRouter": {}
}
